<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <import type="io.mtini.model.TenantModel"/>
        <variable name="tenant" type="TenantModel" />
    </data>

    <FrameLayout
        android:id="@+id/fragment_tenant_detail1"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Test details" />

    <LinearLayout
        android:id="@+id/linearlayout_fragment_tenant_details"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tenant_fragment_position" />


            <TextView android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@{tenant.name}" />

    </LinearLayout>

    </FrameLayout>

</layout>